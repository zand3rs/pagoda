{
  "name": "Popcorn",
  "version": "0.5.0",
  "description": "Popcorn bookmarks.",
  "browser_action": {
    "default_icon": "img/icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/jquery.min.js",
      "js/jquery.md5.js",
      "js/zip/zip.js",
      "js/config.js",
      "js/utils.js",
      "js/popcorn.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "manifest_version": 2
}
