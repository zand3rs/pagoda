{
  "name": "Popcorn",
  "version": "1.0",
  "description": "Popcorn bookmarks.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "unlimitedStorage"
  ],
  "manifest_version": 2
}
